package com.kh.RunnablePre;
			//implements Runnable
public class 러너블연습 implements Runnable {
//필드
	int 초;
	
//메서드
	//필수생성자
	public 러너블연습(int 초) {
		this.초 = 초;
	}
	
	public void run() {
		System.out.println(this.초 + "스레드 실행중");
		try {
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		System.out.println(this.초 + "스레드 종료중");
	}
}
